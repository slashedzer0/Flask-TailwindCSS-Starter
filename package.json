{
  "name": "flask-tailwindcss-starter",
  "version": "1.0.0",
  "description": "A simple Flask starter project with Tailwind CSS",
  "main": "index.js",
  "dependencies": {
    "autoprefixer": "^10.4.19",
    "postcss": "^8.4.38",
    "tailwindcss": "^3.4.4"
  },
  "scripts": {
    "dev": "npx tailwindcss -i ./src/css/tailwind.css -o ./static/css/style.css --watch",
    "minify": "npx tailwindcss -i ./src/css/tailwind.css -o ./static/css/style.css --minify"
  }
}
